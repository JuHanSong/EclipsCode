import java.sql.DataTruncation;
import java.util.Arrays;
import java.util.Scanner;

public class Testr {
	
	static int[] Quiz(int a, int b) {
		return new int[] {a+b, a-b, a/b, a*b};
		
	}
	
	public static void main(String[] args) {
		int[] totalresult = Quiz(10,20);
		
		System.out.println(totalresult[0]);
		System.out.println(totalresult[1]);
		System.out.println(totalresult[2]);
		System.out.println(totalresult[3]);
		
		
		
 	}
}
