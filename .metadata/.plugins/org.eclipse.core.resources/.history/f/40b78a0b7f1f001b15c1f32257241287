import java.sql.DataTruncation;
import java.util.Arrays;
import java.util.Scanner;

public class Testr {
	
	static int addCal(int x) {
		int total = 0;
		for(int i = 1; i <= x; i++) {
			total += i;
		}
		return total;
	}
	
	public static void main(String[] args) {
		System.out.println("1부터 10까지의 합");
		System.out.println(addCal(10));
		
	}
}
