import java.sql.DataTruncation;
import java.util.Arrays;
import java.util.Scanner;

public class Testr {
	
	static int AddNumber(int x) {
		for(int i = 1; i < x; i++) {
			i += i;
		}
	}
	
	public static void main(String[] args) {
		System.out.println("1부터 10까지의 합");
		System.out.println(AddNumber(10));
		
	}
}
